    <!-- https://w3id.org/won/agreement#accepts -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/agreement#accepts">
        <rdfs:domain rdf:resource="https://w3id.org/won/agreement#Agreement"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/won/agreement#Claim"/>
                    <rdf:Description rdf:about="https://w3id.org/won/agreement#Proposal"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">By referencing a Proposal via `msg:accepts` in the ContentGraph of subsequent Mesage, an Agreement is created.</rdfs:comment>
        <rdfs:label>accepts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/agreement#claims -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/agreement#claims">
        <rdfs:domain rdf:resource="https://w3id.org/won/agreement#Claim"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment xml:lang="en">By referencing an earlier Message via `msg:claims` in the ContentGraph of subsequent Mesage, a Claim is created. Claims can be affirmed by the counterpart using `msg:accepts` or rejected using `msg:rejects`.</rdfs:comment>
        <rdfs:label>claims</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/agreement#proposes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/agreement#proposes">
