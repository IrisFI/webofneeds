        <rdfs:range rdf:resource="https://w3id.org/won/agreement#Agreement"/>
        <rdfs:comment xml:lang="en">By referencing an Agreement via `msg:proposesToCancel` in the ContentGraph of subsequent Mesage, a Proposal to cancel an Agreement is created.</rdfs:comment>
        <rdfs:label>proposesToCancel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/agreement#rejects -->


