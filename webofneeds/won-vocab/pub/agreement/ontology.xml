        <rdfs:range rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment xml:lang="en">By referencing a Message via `msg:accepts` in the ContentGraph of subsequent Mesage, a Proposal is created.</rdfs:comment>
        <rdfs:label>proposes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/agreement#proposesToCancel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/agreement#proposesToCancel">
