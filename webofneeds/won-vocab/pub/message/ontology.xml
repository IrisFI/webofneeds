<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/won/message#"
     xml:base="https://w3id.org/won/message"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/">
    <owl:Ontology rdf:about="https://w3id.org/won/message">
        <owl:versionIRI rdf:resource="https://w3id.org/won/message/1.0"/>
        <owl:imports rdf:resource="https://w3id.org/won/core/1.0"/>
        <dc:abstract xml:lang="en">The Web of Needs Message Ontology is used to model the Messages used to publish or modify Atoms, to establish Connections, and to communicate between Atom owners.</dc:abstract>
        <dc:creator>Fabian Salcher</dc:creator>
        <dc:creator>Florian Kleedorfer</dc:creator>
        <dc:creator>Yana Panchenko</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-12-13</dc:date>
        <dc:title xml:lang="en">Web of Needs Message Ontology</dc:title>
        <vann:preferredNamespacePrefix>msg</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">Ontology for describing messages in the Web of Needs project.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/abstract"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/won/message#containsEnvelope -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#containsEnvelope">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#EnvelopeGraph"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#EnvelopeGraph"/>
        <rdfs:label>containsEnvelope</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#containsSignature -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#containsSignature">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#EnvelopeGraph"/>
        <rdfs:range rdf:resource="http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature"/>
        <rdfs:label>containsSignature</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#content -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#content">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#ContentGraph"/>
        <rdfs:comment xml:lang="en">Points to an RDF graph URI that contains the message payload.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>content</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#correspondingRemoteMessage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#correspondingRemoteMessage">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment xml:lang="en">Points to another message. Messages always exist in two copies: one on the sender&apos;s WoN node and one on the receiver&apos;s WoN node. The receiver&apos;s WoN node copies the message, mints a new URI for it and points to the original message using &apos;msg:correspondingRemoteMessage&apos;.</rdfs:comment>
        <rdfs:label>correspondingRemoteMessage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#messageType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#messageType">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Indicates the type of the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>messageType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#previousMessage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#previousMessage">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:label>previousMessage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#recipient -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#recipient">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/won/core#Atom"/>
                    <rdf:Description rdf:about="https://w3id.org/won/core#Connection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Indicates the receiver of the message, either Socket or Connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>recipient</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#recipientAtom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#recipientAtom">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Atom"/>
        <rdfs:comment xml:lang="en">Indicates the Atom that &apos;contains&apos; the receiver of the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>recipientAtom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#recipientNode -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#recipientNode">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Node"/>
        <rdfs:comment xml:lang="en">Indicates the WoN Node the message recipient is hosted on.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>recipientNode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#sender -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#sender">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/won/core#Atom"/>
                    <rdf:Description rdf:about="https://w3id.org/won/core#Connection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Indicates the sender of the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>sender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#senderAtom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#senderAtom">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Atom"/>
        <rdfs:comment xml:lang="en">Indicates the Atom that &apos;contains&apos; the sender of the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>senderAtom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#senderNode -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#senderNode">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Node"/>
        <rdfs:comment xml:lang="en">Indicates the WoN Node the message sender is hosted on.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>senderNode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#signedGraph -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#signedGraph">
        <rdfs:domain rdf:resource="http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature"/>
        <rdfs:range rdf:resource="https://w3id.org/won/message#MessageGraph"/>
        <rdfs:label>signedGraph</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/won/message#hash -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/won/message#hash">
        <rdfs:domain rdf:resource="http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The message hash.</rdfs:comment>
        <rdfs:label>hash</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/won/message#protocolVersion -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/won/message#protocolVersion">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>protocolVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/won/message#publicKeyFingerprint -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/won/message#publicKeyFingerprint">
        <rdfs:domain rdf:resource="http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The fingerprint of the sender&apos;s public key (SHA-256 hash).</rdfs:comment>
        <rdfs:label>publicKeyFingerprint</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/won/message#receivedTimestamp -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/won/message#receivedTimestamp">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment xml:lang="en">Holds the timestamp assigned when the WoN node received and saved the message.</rdfs:comment>
        <rdfs:label>receivedTimestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/won/message#sentTimestamp -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/won/message#sentTimestamp">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment xml:lang="en">Holds the timestamp assigned when the WoN node created and sent the message.</rdfs:comment>
        <rdfs:label>sentTimestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature -->


    <owl:Class rdf:about="http://icp.it-risk.iwvi.uni-koblenz.de/ontologies/signature.owl#Signature"/>
    


    <!-- https://w3id.org/won/core#Atom -->


    <owl:Class rdf:about="https://w3id.org/won/core#Atom"/>
    


    <!-- https://w3id.org/won/core#Connection -->


    <owl:Class rdf:about="https://w3id.org/won/core#Connection"/>
    


    <!-- https://w3id.org/won/core#Node -->


    <owl:Class rdf:about="https://w3id.org/won/core#Node"/>
    


    <!-- https://w3id.org/won/message#ContentGraph -->


    <owl:Class rdf:about="https://w3id.org/won/message#ContentGraph">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#MessageGraph"/>
        <rdfs:label>ContentGraph</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#EnvelopeGraph -->


    <owl:Class rdf:about="https://w3id.org/won/message#EnvelopeGraph">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#MessageGraph"/>
        <rdfs:comment xml:lang="en">Used to define the type of an RDF graph URI so as to explicitly mark it as an envelope graph. An Envelope contains the addressing information.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>EnvelopeGraph</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#FromExternal -->


    <owl:Class rdf:about="https://w3id.org/won/message#FromExternal">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment>A Message received from another WoN node.</rdfs:comment>
        <rdfs:label>FromExternal</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#FromOwner -->


    <owl:Class rdf:about="https://w3id.org/won/message#FromOwner">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment>A Message generated by an Atom&apos;s owner.</rdfs:comment>
        <rdfs:label>FromOwner</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#FromSystem -->


    <owl:Class rdf:about="https://w3id.org/won/message#FromSystem">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:comment>A Message generated by the WoN node (i.e., the &apos;System&apos;).</rdfs:comment>
        <rdfs:label>FromSystem</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#Message -->


    <owl:Class rdf:about="https://w3id.org/won/message#Message">
        <rdfs:comment>Superclass for messages. They fall into three categories: FromOwner, FromSystem and FromExternal.</rdfs:comment>
        <rdfs:label>Message</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#MessageGraph -->


    <owl:Class rdf:about="https://w3id.org/won/message#MessageGraph">
        <rdfs:comment xml:lang="en">An RDF graph that belongs to a Message</rdfs:comment>
        <rdfs:label>MessageGraph</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#MessageType -->


    <owl:Class rdf:about="https://w3id.org/won/message#MessageType">
        <rdfs:comment xml:lang="en">Class containing all WoN message types.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>MessageType</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/won/message#ResponseType -->


    <owl:Class rdf:about="https://w3id.org/won/message#ResponseType">
        <rdfs:subClassOf rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Indicates the (success/error) state of a response message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ResponseType</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/won/message#ActivateMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ActivateMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that activates an Atom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ActivateMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#AtomMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#AtomMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Message sent between an atom&apos;s owner and the WoN node hosting the atom. The message is informative only
                   and has no side effect The message is not routed to any other participant.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>AtomMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ChangeNotificationMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ChangeNotificationMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent from a WoN Node to indicate that the remote Atom has changed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ChangeNotificationMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#CloseMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#CloseMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that closes a Connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>CloseMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ConnectMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ConnectMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that is sent from Atom A to Atom B in order to create a new Connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ConnectMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ConnectionMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ConnectionMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Message sent via an established connection. This type is to be used in Socket-specific protocols.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ConnectionMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#CreateMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#CreateMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent to a WoN Node in order to create a new Atom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>CreateMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#DeactivateMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#DeactivateMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that deactivates an Atom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>DeactivateMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#FailureResponse -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#FailureResponse">
        <rdf:type rdf:resource="https://w3id.org/won/message#ResponseType"/>
        <rdfs:comment xml:lang="en">Indicates an error occurred while processing the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>FailureResponse</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#HintMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#HintMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent by a matching service to an Atom in order to inform its owner of other Atoms possibly interesting for establishing a connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>HintMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#OpenMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#OpenMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of Message used to (re)open an existing Connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>OpenMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ReplaceMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ReplaceMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent to a WoN Node in order replace an Atom&apos;s content.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ReplaceMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#SuccessResponse -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#SuccessResponse">
        <rdf:type rdf:resource="https://w3id.org/won/message#ResponseType"/>
        <rdfs:comment xml:lang="en">Indicates that the message was processed successfully.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>SuccessResponse</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/won/message#EnvelopeGraph"/>
            <rdf:Description rdf:about="https://w3id.org/won/message#Message"/>
            <rdf:Description rdf:about="https://w3id.org/won/message#MessageType"/>
            <rdf:Description rdf:about="https://w3id.org/won/message#ResponseType"/>
        </owl:members>
