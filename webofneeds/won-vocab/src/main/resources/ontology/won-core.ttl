[ rdf:type owl:AllDisjointClasses ;
  owl:members ( <http://www.w3.org/ns/ldp#CompositeContainer>
                :Atom
                :AtomState
                :Connection
                :ConnectionState
                :Flag
                :Node
                :Socket
                :SocketDefinition
                :UriPrefixSpecification
                :WonProtocol
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers rdf:nil
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Active
                        :Closed
                        :Connected
                        :Inactive
                        :NoHintForCounterpart
                        :NoHintForMe
                        :RequestReceived
                        :RequestSent
                        :Suggested
                        :UsedForTesting
                        :WhatsAround
                        :WhatsNew
                      )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( :additionalData
                :atomState
                :compatibleSocketDefinition
                :connectionState
                :connections
                :defaultSocket
                :derivesAtomProperty
                :derivesInverseAtomProperty
                :flag
                :messageContainer
                :seeks
                :socket
                :socketDefinition
                :supportsWonProtocolImpl
                :targetAtom
                :targetConnection
                :targetSocket
                :uriPrefixSpecification
              )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( :activeMQAtomProtocolQueueName
                :activeMQOwnerProtocolQueueName
                :atomUriPrefix
                :autoOpen
                :brokerUri
                :connectionUriPrefix
                :doNotMatchAfter
                :doNotMatchBefore
                :eventUriPrefix
                :socketCapacity
                :tag
              )
] .


